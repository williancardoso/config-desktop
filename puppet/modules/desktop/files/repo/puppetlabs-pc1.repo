[puppetlabs-pc1]
name=Puppet Labs PC1 Repository fedora f22 - $basearch
baseurl=http://yum.puppetlabs.com/fedora/f22/PC1/$basearch
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs-PC1
enabled=1
gpgcheck=1

[puppetlabs-pc1-source]
name=Puppet Labs PC1 Repository fedora f22 - Source
baseurl=http://yum.puppetlabs.com/fedora/f22/PC1/SRPMS
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppetlabs-PC1
failovermethod=priority
enabled=0
gpgcheck=1

